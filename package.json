    "dead-code",
    "unused-code",
    "code-quality",
    "code-cleanup",
    "refactoring",
    "technical-debt",
    "ci",
    "continuous-integration",
    "github-actions",
    "gitlab-ci",
    "circleci",
    "devtools",
    "developer-tools",
    "linting",
    "code-review",
    "typescript",
    "javascript",
    "nodejs",
    "npm",
    "npx",
    "zero-dependencies",
    "cli",
    "command-line"
  ],
  "author": {
    "name": "Flagwatch Contributors"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dxmari/flagwatch.git"
  },
